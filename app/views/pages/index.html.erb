<!-- navbar fade in/out -->
<script type="text/javascript">
(function($) {         
    $(document).ready(function(){                    
        $(window).scroll(function(){                          
            if ($(this).scrollTop() > screen.height) {
                $('#navbar').fadeIn(500);
            } else {
                $('#navbar').hide();
            }
        });
    });
})(jQuery);
</script>

